    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-load-config": "^3.1.3",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "prop-types": "^15.8.1"
