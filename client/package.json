  "scripts2": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "style": "echo should do something",
    "test": "craco test",
    "lint": "npx eslint client/src",
    "eject": "react-scripts eject",
    "electron": "app/electron .",
    "dev": "PORT=3000 nf start",
    "dev:electron": "ELECTRON_START_URL=http://localhost:3000 npx app/electron .",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "postinstall": "electron-builder install-app-deps",
    "prebundle": "rimraf dist && yarn run style",
    "prebundle:local": "rimraf dist",
    "dist": "electron-builder",
    "bundle": "electron-builder -mwl --publish never",
    "bundle:local": "NODE_ENV=development electron-builder -m",
    "preprod": "cross-var yarn bundle -c.extraMetadata.version=$npm_package_version-snapshot.$(date +\"%s\")",
    "preprod:local": "NODE_ENV=development cross-var yarn bundle:local -c.buildVersion=$(date +\"%s\") -c.extraMetadata.version=$npm_package_version-snapshot.$(date +\"%s\")",
    "release": "cross-var yarn bundle -c.buildVersion=$npm_package_version-$CI_COMMIT_SHORT_SHA -c.extraMetadata.version=$npm_package_version"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html"
    ]
  },
  "devDependencies": {
